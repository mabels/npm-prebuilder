  "scripts": {
    "clean": "npx lerna clean --yes && npx rimraf node_modules && npx rimraf app.properties && npx lerna run clean --no-bail",
    "build": "npx myaudi-ci-tools-lerna build",
    "dev": "npx myaudi-ci-tools-dev",
    "compose": "npx lerna run compose",
    "init": "npx myaudi-ci-tools-lerna init --concurrency=1",
    "purge": "npx myaudi-ci-tools-purge",
    "reinstall": "npx lerna clean --yes && npx rimraf node_modules && npx yarn install --pure-lockfile && npx lerna bootstrap",
    "resetup": "npx yarn run purge && npx yarn run setup",
    "setup": "npx yarn install --pure-lockfile && npx lerna run init && npx lerna bootstrap && npx lerna run build",
    "start": "npx lerna run start --parallel --stream",
    "test": "npx myaudi-ci-tools-lerna test --verbose",
    "watch": "npx myaudi-ci-tools-watch-global --baseDir .",
    "postcommit": "git update-index --again",
    "precommit": "lint-staged",
    "release": "npx release"
  },
