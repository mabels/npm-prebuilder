{
  "name": "jsdom-simulant",
  "description": "Simulated DOM events for automated testing",
  "version": "1.1.2",
  "author": {
    "name": "Rich Harris"
  },
  "contributors": [
    {
      "name": "Luke Bayes"
    }
  ],
  "license": "MIT",
  "repository": "https://github.com/lukebayes/simulant/tree/jsdom",
  "main": "dist/jsdom-simulant.umd.js",
  "jsnext:main": "dist/jsdom-simulant.es.js",
  "scripts": {
    "prepublish": "npm run lint && rm -rf dist && npm test && npm run build:es",
    "build": "npm run build:umd && npm run build:es",
    "build:umd": "rollup -c -f umd -o dist/jsdom-simulant.umd.js",
    "build:es": "rollup -c -f es6 -o dist/jsdom-simulant.es.js",
    "test": "node test/run.js",
    "pretest": "npm run build:umd",
    "lint": "eslint src"
  },
  "devDependencies": {
    "chalk": "^1.1.3",
    "eslint": "^2.8.0",
    "nightmare": "^2.3.4",
    "rollup": "^0.26.0",
    "rollup-plugin-buble": "^0.7.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
